# GitHub Repo Creation Properties
github_username="<username>"
# github_use_ssh: true will use ssh, false will use https
github_use_ssh="true"
# github repo format: "entity_id/repo-name.git" entity_id is the github username or organization name
github_template_repo="<username_or_org>/<repo-template-name>"
github_new_repo="<username_or_org>/<new-repo-name>"
# project_visibility: public, private, internal
github_new_repo_visibility="<visibility>" 

# Dev Environment Provision Properties
azd_dev_env_provision="<true_or_false>"
azd_dev_env_name="<dev_env_name>"
azd_dev_env_subscription="<subscription_id>"
azd_dev_env_location="<location>"